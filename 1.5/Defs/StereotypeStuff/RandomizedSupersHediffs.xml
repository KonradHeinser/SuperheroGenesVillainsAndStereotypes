<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <HediffDef Abstract="True" Name="SHG_XenotypeRandomizerHediff">
        <label>generating new genes</label>
        <description>This super will generate new genes once you STOP LOOKING AT DEVMODE STUFF AND UNPAUSE YOU CHEATER. Unless you're a fellow modder, in which case welcome! You won't get much use from this tooltip, but the RandomizedSuperHediffs.xml file contains the randomization data for all super stereotypes.</description>
        <hediffClass>HediffWithComps</hediffClass>
        <stages>
			<li>
                <becomeVisible>false</becomeVisible>
            </li>
        </stages>
    </HediffDef>

    <HediffDef ParentName="SHG_XenotypeRandomizerHediff">
        <defName>SHG_RandomMundaneSuper</defName>       
        <comps>
            <li Class="SuperHeroGenesBase.HediffCompProperties_GainRandomGeneSet">
                <alwaysRemovedGenes>
                    <li>SHG_RandomMundaneSuper</li>
                </alwaysRemovedGenes>
                <geneSets>
                    <li> <!--Basic-->
                        <geneSet />
                    </li>
                    <li> <!--Brainiac-->
                        <geneSet>
                            <li>SuperIntelligence</li>
                            <li>SuperPerception</li>
                            <li>UnsuperHealing</li>
                            <li>AntisuperHealing</li>
                            <li>Body_Thin</li>
                        </geneSet>
                    </li>
                    <li> <!--Influencer-->
                        <geneSet>
                            <li>SuperPerception</li>
                            <li>UnsuperDurability</li>
                            <li>UnsuperPrecision</li>
                            <li>UnsuperStrength</li>
                            <li>AntisuperStrength</li>
                            <li>SuperPersuasion</li>
                            <li>Body_Thin</li>
                        </geneSet>
                    </li>
                </geneSets>
            </li>
        </comps>
    </HediffDef>

